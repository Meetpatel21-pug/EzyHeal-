# SMS Configuration
# Copy this file to config.env and fill in your credentials

# SMS Provider: 'twilio', 'fast2sms', 'msg91', or 'console' (for testing)
SMS_PROVIDER=console

# Twilio Configuration (if using Twilio)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Fast2SMS Configuration (if using Fast2SMS - Indian service)
FAST2SMS_API_KEY=your_fast2sms_api_key
FAST2SMS_SENDER_ID=EZYHEAL

# MSG91 Configuration (if using MSG91 - Indian service)
MSG91_AUTH_KEY=your_msg91_auth_key
MSG91_SENDER_ID=EZYHEL
